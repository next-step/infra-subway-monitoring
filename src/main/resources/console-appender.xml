<?xml version="1.0" encoding="UTF-8"?>

<included>
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <!--   해당 로깅의 패턴을 설정   -->
        <encoder>
            <charset>utf8</charset>
            <Pattern>
                %d{yyyy-MM-dd HH:mm:ss.SSS} %thread %-6level %logger - %m%n
            </Pattern>
        </encoder>
    </appender>
</included>
